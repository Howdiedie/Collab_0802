@model List<Collab.Models.NotifyWithMember>
<section class="">
    <div class="row">
        <div class="col scroller py-5" style="height: 87vh;">
            <ul class="timeline-with-icons">
                @foreach (var notify in Model) {
                    <li class="timeline-item mb-5">
                        <span class="timeline-icon">
                            <i class="fas fa-hand-holding-usd text-primary fa-sm fa-fw"></i>
                        </span>
                        <h4 class="fw-bold">
                            @notify.MemberName
                            @notify.NotifyAction 了
                            @notify.NotifyType &nbsp;
                            「@notify.ActionName」
                        </h4>
                        <p class="text-muted mb-2 fw-bold">
                            @notify.NotifyDate
                        </p>
                    </li>
                }
            </ul>
        </div>
        <div class="col">
            <img class="notifyImg"
                 src="~/img/Notify/—Pngtree—people make puzzles concept team_5356575.png" alt="">
        </div>
    </div>
</section>
<link rel="stylesheet" href="~/css/Notify.css" />